{
  "name": "onyami-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "typecheck": "tsc --noEmit -w --skipLibCheck",
    "build": "babel index.js -d dist",
    "start": "NODE_PATH=dist node dist/index.js",
    "dev": "NODE_PATH=src nodemon src/index.ts",
    "db:init": "scripts/db/init.sh",
    "db:start": "docker run --rm --name onyami-postgres -e POSTGRES_PASSWORD=onyami -d -p 127.0.0.1:5432:5432 -v $(pwd)/docker-volumes/postgres:/var/lib/postgresql/data postgres",
    "db:stop": "docker stop onyami-postgres",
    "migrate:make": "knex migrate:make --knexfile src/config/knexfile.js --env local",
    "migrate:latest": "knex migrate:latest --knexfile src/config/knexfile.js --env local",
    "migrate:up": "knex migrate:up --knexfile src/config/knexfile.js --env local",
    "migrate:rollback": "knex migrate:rollback --knexfile src/config/knexfile.js --env local",
    "migrate:redo": "npm run migrate:rollback && npm run migrate:up",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "graphql:codegen:watch": "graphql-codegen --config src/services/graphql/codegen.yml --watch"
  },
  "keywords": [],
  "author": "lionel tay",
  "license": "ISC",
  "dependencies": {
    "@types/snake-case": "^1.1.2",
    "@types/valid-url": "^1.0.2",
    "apollo-server": "^2.6.1",
    "apollo-server-express": "^2.6.1",
    "camelcase": "^5.3.1",
    "express": "^4.17.1",
    "firebase-admin": "^8.0.0",
    "graphql": "^14.3.1",
    "graphql-fields": "^2.0.3",
    "knex": "^0.19.5",
    "pg": "^7.11.0",
    "ramda": "^0.26.1",
    "snake-case": "^2.1.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@graphql-codegen/cli": "^1.2.0",
    "@graphql-codegen/typescript": "1.2.0",
    "@graphql-codegen/typescript-resolvers": "1.2.0",
    "@playlyfe/gql": "^2.6.1",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/graphql-fields": "^1.3.0",
    "@types/ramda": "^0.26.9",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1"
  }
}
